<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:pe="http://primefaces.org/ui/extensions"
	xmlns:c="http://java.sun.com/jstl/core">
<h:body>
	<ui:composition template="template/common/commonLayout.xhtml">
		<ui:define name="west">
			<p:layoutUnit position="west" size="250" header="Opciones"
				resizable="true" closable="false" collapsible="false" id="west">
				<h:form id="frmMenuLogin">
					<p:panelMenu>
						<!-- style="width:200px" -->
						<c:forEach var="currOpc" items="#{MBUsuarios.currentOpciones}">
							<p:submenu label="#{currOpc.descrip}">
								<c:forEach var="currOpcChild" items="#{currOpc.childsAsArray}">
									<p:menuitem value="&nbsp;&nbsp;&nbsp;#{currOpcChild.descrip}"
										actionListener='#{menuBean.goCatalog(currOpcChild.url,currOpcChild.opcName)}'
										update=":pgPrincipalLogin">
									</p:menuitem>
								</c:forEach>
							</p:submenu>
						</c:forEach>
					</p:panelMenu>
				</h:form>
			</p:layoutUnit>
		</ui:define>
		<ui:define name="content">
			<h:panelGroup id="pgPrincipalLogin">
			 <p:growl id="growl" showDetail="true" sticky="true" />  
			 <ui:include src="/pages/prueba.xhtml"/>
			</h:panelGroup>
		</ui:define>
	</ui:composition>
</h:body>
</html>