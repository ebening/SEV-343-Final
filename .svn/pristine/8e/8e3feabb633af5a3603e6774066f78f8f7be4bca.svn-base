package com.adinfi.seven.business.domain;

/**
 * Entidad del catalogo CAT_ACTIVITY
 * 
 * @author Miguel Ramirez
 */
public class ActivObj {

	int id;
	String name;
	String descripcion;
	int durDias;
	int durHoras;
	int diasAntes;
	int idRol;
	int isFlujo;
	int idNotif;
	String vigencia;
	String code;
	String closeable;
	int isGeneral;
	TblCampanaActividades campanaActividad= null;

	public ActivObj() {
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public int getDurDias() {
		return durDias;
	}

	public void setDurDias(int durDias) {
		this.durDias = durDias;
	}

	public int getDurHoras() {
		return durHoras;
	}

	public void setDurHoras(int durHoras) {
		this.durHoras = durHoras;
	}

	public int getDiasAntes() {
		return diasAntes;
	}

	public void setDiasAntes(int diasAntes) {
		this.diasAntes = diasAntes;
	}

	public int getIdRol() {
		return idRol;
	}

	public void setIdRol(int idRol) {
		this.idRol = idRol;
	}

	public int getIsFlujo() {
		return isFlujo;
	}

	public void setIsFlujo(int isFlujo) {
		this.isFlujo = isFlujo;
	}

	public int getIdNotif() {
		return idNotif;
	}

	public void setIdNotif(int idNotif) {
		this.idNotif = idNotif;
	}

	public String getVigencia() {
		return vigencia;
	}

	public void setVigencia(String vigencia) {
		this.vigencia = vigencia;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getCloseable() {
		return closeable;
	}

	public void setCloseable(String closeable) {
		this.closeable = closeable;
	}

	public int getIsGeneral() {
		return isGeneral;
	}

	public void setIsGeneral(int isGeneral) {
		this.isGeneral = isGeneral;
	}

	public TblCampanaActividades getCampanaActividad() {
		return campanaActividad;
	}

	public void setCampanaActividad(TblCampanaActividades campanaActividad) {
		this.campanaActividad = campanaActividad;
	}
}
