
.ad-button { 
	display: inline-block; 
}
 /* the overflow property removes extra width in IE */
 
 .ui-buttonset .ad-button { margin-left: 0; margin-right: -.3em; }
 
/*	
 * ui-button-text 
 * ui-button-text-only
 */

/*button text element */
.ad-button .ad-button-text { display: block; line-height: 1.4;  }
.ad-button-text-only .ad-button-text { padding: .3em 1em; }
.ui-button-icon-only .ad-button-text, .ui-button-icons-only .ad-button-text { padding: .4em; text-indent: -9999999px; }
.ui-button-text-icon-primary .ad-button-text, .ui-button-text-icons .ad-button-text { padding: .4em 1em .4em 2.1em; }
.ui-button-text-icon-secondary .ad-button-text, .ui-button-text-icons .ad-button-text { padding: .4em 2.1em .4em 1em; }
.ui-button-text-icons .ad-button-text { padding-left: 2.1em; padding-right: 2.1em; }
/* no icon support for input elements, provide padding by default */
input.ad-button { padding: .0em 1em; }

/*
 * ui-widget
 */ 
.ad-widget { 
	font-family: Verdana,Arial,sans-serif/*{ffDefault}*/; 
	font-size: 12px/*{fsDefault}*/; 
}

/*
 * ui-state-default
 */
.ad-state-default { 
	border: 1px solid #d3d3d3/*{borderColorDefault}*/; 
	background: #e6e6e6/*{bgColorDefault}*/
    url(#{resource['images:degradado_btn_2.png']})
				50%/*{bgDefaultXPos}*/ 
				50%/*{bgDefaultYPos}*/ 
				repeat-x/*{bgDefaultRepeat}*/; 
	font-weight: normal/*{fwDefault}*/; 
	/*color: #555555*//*{fcDefault}*/; 
	
	font-size: 11px !important;
	color: white;
	-ms-border-radius: 5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	-khtml-border-radius: 5px;  
	border-radius: 5px;
}

/*
 * ui-corner-all 
 */
.ad-corner-all, .ad-corner-top, .ad-corner-left, .ad-corner-tl { -moz-border-radius-topleft: 8px/*{cornerRadius}*/; -webkit-border-top-left-radius: 8px/*{cornerRadius}*/; -khtml-border-top-left-radius: 8px/*{cornerRadius}*/; border-top-left-radius: 8px/*{cornerRadius}*/; }
.ad-corner-all, .ad-corner-top, .ad-corner-right, .ad-corner-tr { -moz-border-radius-topright: 8px/*{cornerRadius}*/; -webkit-border-top-right-radius: 8px/*{cornerRadius}*/; -khtml-border-top-right-radius: 8px/*{cornerRadius}*/; border-top-right-radius: 8px/*{cornerRadius}*/; }
.ad-corner-all, .ad-corner-bottom, .ad-corner-left, .ad-corner-bl { -moz-border-radius-bottomleft: 8px/*{cornerRadius}*/; -webkit-border-bottom-left-radius: 8px/*{cornerRadius}*/; -khtml-border-bottom-left-radius: 8px/*{cornerRadius}*/; border-bottom-left-radius: 8px/*{cornerRadius}*/; }
.ad-corner-all, .ad-corner-bottom, .ad-corner-right, .ad-corner-br { -moz-border-radius-bottomright: 8px/*{cornerRadius}*/; -webkit-border-bottom-right-radius: 8px/*{cornerRadius}*/; -khtml-border-bottom-right-radius: 8px/*{cornerRadius}*/; border-bottom-right-radius: 8px/*{cornerRadius}*/; }




