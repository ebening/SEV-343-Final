package com.adinfi.seven.business.domain;

import java.io.Serializable;

public class ArqSevenTreeNode implements Serializable{
	private TblCampana tblCampana;
	private TblCampanaProgramas tblCampanaProgramas;
	private TblMecanica tblMecanica;
	private String name;
	private String label;
	private String page;
	
	public ArqSevenTreeNode(String name, String label, TblCampana tblCampana, TblCampanaProgramas tblCampanaProgramas, TblMecanica tblMecanica) {
		this.name = name;
		this.label = label;
		this.tblCampana = tblCampana;
		this.tblCampanaProgramas = tblCampanaProgramas;
		this.tblMecanica = tblMecanica;
	}
	
	public TblCampana getTblCampana() {
		return tblCampana;
	}
	
	public void setTblCampana(TblCampana tblCampana) {
		this.tblCampana = tblCampana;
	}
	
	public TblCampanaProgramas getTblCampanaProgramas() {
		return tblCampanaProgramas;
	}

	public void setTblCampanaProgramas(TblCampanaProgramas tblCampanaProgramas) {
		this.tblCampanaProgramas = tblCampanaProgramas;
	}

	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getLabel() {
		return label;
	}
	
	public void setLabel(String label) {
		this.label = label;
	}
	
	public String getPage() {
		return page;
	}

	public void setPage(String page) {
		this.page = page;
	}

	public TblMecanica getTblMecanica() {
		return tblMecanica;
	}

	public void setTblMecanica(TblMecanica tblMecanica) {
		this.tblMecanica = tblMecanica;
	}

	@Override
	public String toString(){
		return this.name;
	}
}
