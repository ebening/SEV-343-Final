		function minmax(event, value, min, max) 
        {
        	event = event || window.event;
            if (event.keyCode &lt; 48 || event.keyCode &gt; 57) return 1;
            if(min > parseInt(value) || isNaN(parseInt(value))) 
                return 1; 
            else if(parseInt(value) > max) 
                return 99; 
            else return value;
        }
	</script>
	
