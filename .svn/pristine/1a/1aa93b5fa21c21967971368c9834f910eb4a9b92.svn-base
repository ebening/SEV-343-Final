<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:c="http://java.sun.com/jsp/jstl/core">

<h:head></h:head>
<body>
	<table cellpadding="0" cellspacing="0">
		<tr>

			<c:forEach var="currOpc1" items="#{MBUsuarios.opciones}">


				<c:if test="#{currOpc1.url !=null and currOpc1.url.length()>0}">


					<h:commandButton action="#{currOpc1.url}"
						title="#{currOpc1.opcName}" styleClass="botonCabezera"
						style="background-image:url('../../resources/images/#{currOpc1.fileImage}'); 
						background-size:30px 30px; background-repeat:no-repeat; background-position: center"
						actionListener="#{MBUsuarios.setCurrentOpcionesView(currOpc1.childsAsArray)}" />

				</c:if>

				<c:if test="#{currOpc1.url ==null or currOpc1.url.length()==0}">

					<h:commandButton action="#{currOpc1.url}"
						title="#{currOpc1.opcName}" styleClass="botonCabezera"
						style="background-image:url('../../resources/images/#{currOpc1.fileImage}');
						background-size:30px 30px; background-repeat:no-repeat;background-position: center"
						actionListener="#{MBUsuarios.setCurrentOpcionesView(currOpc1.childsAsArray)}" />

				</c:if>
			</c:forEach>

		</tr>
	</table>
</body>
</html>
