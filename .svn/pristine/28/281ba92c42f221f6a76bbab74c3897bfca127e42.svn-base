       					 </p:column>
			<p:column headerText="Periodo" style="width:24%">  
         			  		 #{campana.periodo}  
      					  </p:column>
			<p:column headerText="Responsable" style="width:24%">  
           				 #{campana.responsable}  
       				 </p:column>
			<p:column headerText="AÃ±o" style="width:24%">  
        			    #{campana.year}  
       				</p:column>
		</p:dataTable>
	</h:form>
</body>
</html>
