						<tr>
							<th>
								<table id="tbl_menu_principal" cellpadding="0" cellspacing="0">
									<tr>
										<td id="td_menu_home" title="Inicio">
											<h:graphicImage value="../resources/images/home.png" />
										</td>
										<td id="td_menu_planeacion" title="Planeación">
											<h:graphicImage value="../resources/images/planeacion.png" />
										</td>
										<td id="td_menu_analisis" title="Análisis">
											<h:graphicImage value="../resources/images/analisis.png" />
										</td>
										<td id="td_menu_arquitectura" title="Arquitectura">
											<h:graphicImage value="../resources/images/arquitectura.png" />
										</td>
										<td id="td_menu_ejecucion" title="Ejecución">
											<h:graphicImage value="../resources/images/ejecucion.png" />
										</td>
										<td id="td_menu_tableroControl" title="Tablero de Control">
											<h:graphicImage value="../resources/images/tablero_de_control.png" />
