package com.adinfi.seven.persistence.dto;

/**
 * @author OMAR
 *
 */
public class DepartamentoDTO {
	
	private int id;
	
	private String codigo;
	
	private boolean esMercancia;

	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * @return the codigo
	 */
	public String getCodigo() {
		return codigo;
	}

	/**
	 * @param codigo the codigo to set
	 */
	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}

	/**
	 * @return the esMercancia
	 */
	public boolean isEsMercancia() {
		return esMercancia;
	}

	/**
	 * @param esMercancia the esMercancia to set
	 */
	public void setEsMercancia(boolean esMercancia) {
		this.esMercancia = esMercancia;
	}
	
	

}
