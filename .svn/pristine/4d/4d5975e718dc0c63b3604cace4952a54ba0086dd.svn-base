<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
<body>
	<p:panelGrid style="border:none;" columns="2">
		<p:outputLabel value="CampaÃ±as: " />
		<p:inputText value="#{campanaController.current.tblCampana.nombre}"
			disabled="true" />
		<p:outputLabel value="Medio: " />
		<p:selectOneMenu value="#{campanaController.medio}">
			<f:selectItems value="#{campanaController.medios}" />
			<p:ajax event="change" update="tipoMedio"
				listener="#{campanaController.getTipoMedios}" />
		</p:selectOneMenu>
		<p:outputLabel value="Tipo: " />
		<p:selectOneMenu id="tipoMedio" value="#{campanaController.tipo}">
			<f:selectItems value="#{campanaController.tipos}" />
		</p:selectOneMenu>
		<p:commandButton value="Aceptar" onclick="w_medio.hide()" />
		<p:commandButton value="Cancelar" onclick="w_medio.hide()" />
	</p:panelGrid>
</body>
</html>
